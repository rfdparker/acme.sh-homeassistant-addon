{
  "name": "Acme.sh",
  "version": "2.0.0",
  "slug": "acmesh",
  "description": "Full ACME client to generate and renew a SSL certificate",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "services",
  "map": ["ssl:rw"],
  "boot": "auto",
  "options": {
    "accountemail": null,
    "domain": null,
    "dns": null,
    "dnsEnvVariables": [null],
    "keylength": "ec-256",
    "fullchainfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "server": "zerossl"
  },
  "schema": {
    "accountemail": "str",
    "domain": "str",
    "dns": "str",
    "dnsEnvVariables": [
      {"name": "str", "value": "str"}
    ],
    "keylength": "match(2048|4096|ec-256|ec-384|ec-521)",
    "fullchainfile": "str",
    "keyfile": "str",
    "server": "match(buypass|buypass_test|google|googletest|letsencrypt|letsencrypt_test|sslcom|zerossl)"
  }
}
